{"remainingRequest":"C:\\Polybasic Disk\\University\\Graduation project\\springbootbeua19u3\\src\\main\\resources\\front\\front\\node_modules\\babel-loader\\lib\\index.js!C:\\Polybasic Disk\\University\\Graduation project\\springbootbeua19u3\\src\\main\\resources\\front\\front\\node_modules\\cache-loader\\dist\\cjs.js??ref--1-0!C:\\Polybasic Disk\\University\\Graduation project\\springbootbeua19u3\\src\\main\\resources\\front\\front\\node_modules\\vue-loader\\lib\\index.js??vue-loader-options!C:\\Polybasic Disk\\University\\Graduation project\\springbootbeua19u3\\src\\main\\resources\\front\\front\\src\\pages\\news\\news-detail.vue?vue&type=script&lang=js","dependencies":[{"path":"C:\\Polybasic Disk\\University\\Graduation project\\springbootbeua19u3\\src\\main\\resources\\front\\front\\src\\pages\\news\\news-detail.vue","mtime":1713361872000},{"path":"C:\\Polybasic Disk\\University\\Graduation project\\springbootbeua19u3\\src\\main\\resources\\front\\front\\babel.config.js","mtime":1713361864000},{"path":"C:\\Polybasic Disk\\University\\Graduation project\\springbootbeua19u3\\src\\main\\resources\\front\\front\\node_modules\\cache-loader\\dist\\cjs.js","mtime":499162500000},{"path":"C:\\Polybasic Disk\\University\\Graduation project\\springbootbeua19u3\\src\\main\\resources\\front\\front\\node_modules\\babel-loader\\lib\\index.js","mtime":456789000000},{"path":"C:\\Polybasic Disk\\University\\Graduation project\\springbootbeua19u3\\src\\main\\resources\\front\\front\\node_modules\\cache-loader\\dist\\cjs.js","mtime":499162500000},{"path":"C:\\Polybasic Disk\\University\\Graduation project\\springbootbeua19u3\\src\\main\\resources\\front\\front\\node_modules\\vue-loader\\lib\\index.js","mtime":499162500000}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"names":["data","id","detail","zanType","zanForm","collectType","collectForm","baseUrl","recommendList","categoryList","currentIndex","allList","created","$route","query","$config","getCategoryList","getDetail","getNewsList","getRecommendList","watch","newValue","methods","backClick","history","back","params","page","limit","sort","order","$http","get","then","res","code","x","list","Number","prepDetailClick","$message","error","$router","push","path","nextDetailClick","length","url","localStorage","getItem","toDetail","window","scrollTo","getZan","getCollect","type","userid","tablename","refid","zanClick","post","success","thumbsupnum","obj","name","title","picture","collectClick","storeupnum"],"sources":["src/pages/news/news-detail.vue"],"sourcesContent":["<template>\r\n\t<div :style='{\"padding\":\"30px calc((100% - 1200px)/2) 0\",\"margin\":\"10px auto\",\"alignItems\":\"flex-start\",\"flexWrap\":\"wrap\",\"background\":\"#fff\",\"display\":\"flex\",\"width\":\"100%\",\"position\":\"relative\",\"justifyContent\":\"space-between\"}'>\r\n\t<el-button :style='{\"border\":\"0\",\"cursor\":\"pointer\",\"padding\":\"0 10px\",\"margin\":\"0 5px 0 0\",\"outline\":\"none\",\"color\":\"#333\",\"borderRadius\":\"2px\",\"background\":\"#b4ebcc\",\"width\":\"auto\",\"lineHeight\":\"36px\",\"fontSize\":\"14px\",\"height\":\"36px\"}' type=\"warning\" size=\"mini\" @click=\"backClick\" class=\"el-icon-back\">返回</el-button>\r\n\t<div :style='{\"width\":\"100%\",\"overflow\":\"hidden\",\"flexWrap\":\"wrap\",\"background\":\"#fff\",\"justifyContent\":\"space-between\",\"display\":\"flex\"}'>\r\n\t\t<div :style='{\"margin\":\"10px 0 0\",\"textAlign\":\"center\",\"background\":\"url(http://codegen.caihongy.cn/20230921/eb4b56c7d55142c4b9339e032022bfb5.png) repeat-x center bottom\",\"display\":\"none\",\"width\":\"calc(100% - 0px)\",\"lineHeight\":\"56px\",\"height\":\"66px\"}'>\r\n\t\t\t<div :style='{\"margin\":\"0 auto\",\"color\":\"#333\",\"textAlign\":\"center\",\"background\":\"none\",\"display\":\"inline-block\",\"width\":\"auto\",\"fontSize\":\"20px\",\"fontWeight\":\"500\"}'>公告资讯</div>\r\n\t\t</div>\r\n\t\t<div :style='{\"minHeight\":\"360px\",\"border\":\"1px solid rgb(123, 123, 123)\",\"padding\":\"30px 40px 0\",\"alignItems\":\"flex-start\",\"borderRadius\":\"10px\",\"flexWrap\":\"wrap\",\"background\":\"none\",\"flex\":\"1\",\"display\":\"flex\",\"width\":\"calc(100% - 280px)\",\"position\":\"relative\",\"order\":\"2\"}'>\r\n\t\t\t<div  :style='{\"margin\":\"0 0 10px\",\"color\":\"#333\",\"textAlign\":\"center\",\"width\":\"100%\",\"lineHeight\":\"40px\",\"fontSize\":\"22px\",\"fontWeight\":\"600\"}'>{{detail.title}}</div>\r\n\t\t\t\r\n\t\t\t<div :style='{\"alignItems\":\"center\",\"color\":\"#999\",\"background\":\"none\",\"display\":\"flex\",\"width\":\"100%\",\"fontSize\":\"14px\",\"position\":\"relative\",\"justifyContent\":\"flex-start\"}'>\r\n\t\t\t  <div :style='{\"padding\":\"0\",\"margin\":\"0 10px 0 0\",\"position\":\"absolute\",\"right\":\"0\"}'>\r\n\t\t\t    <span class=\"icon iconfont icon-shijian21\" :style='{\"margin\":\"0 2px 0 0\",\"lineHeight\":\"1.5\",\"fontSize\":\"inherit\",\"color\":\"inherit\",\"display\":\"none\"}'></span>\r\n\t\t\t    <span :style='{\"color\":\"inherit\",\"lineHeight\":\"1.5\",\"fontSize\":\"inherit\"}'>发布时间：{{detail.addtime}}</span>\r\n\t\t\t  </div>\r\n\t\t\t  <div :style='{\"padding\":\"0\",\"margin\":\"0 10px 0 0\"}'>\r\n\t\t\t    <span class=\"icon iconfont icon-geren17\" :style='{\"margin\":\"0 2px 0 0\",\"lineHeight\":\"1.5\",\"fontSize\":\"inherit\",\"color\":\"inherit\"}'></span>\r\n\t\t\t    <span :style='{\"color\":\"inherit\",\"lineHeight\":\"1.5\",\"fontSize\":\"inherit\"}'>{{detail.name}}</span>\r\n\t\t\t  </div>\r\n\t\t\t  <div :style='{\"padding\":\"0\",\"margin\":\"0 10px 0 0\"}'>\r\n\t\t\t    <span class=\"icon iconfont icon-zan10\" :style='{\"margin\":\"0 2px 0 0\",\"lineHeight\":\"1.5\",\"fontSize\":\"inherit\",\"color\":\"inherit\"}'></span>\r\n\t\t\t    <span :style='{\"color\":\"inherit\",\"lineHeight\":\"1.5\",\"fontSize\":\"inherit\"}'>{{detail.thumbsupnum}}</span>\r\n\t\t\t  </div>\r\n\t\t\t  <div :style='{\"padding\":\"0\",\"margin\":\"0 10px 0 0\"}'>\r\n\t\t\t    <span class=\"icon iconfont icon-cai15\" :style='{\"margin\":\"0 2px 0 0\",\"lineHeight\":\"1.5\",\"fontSize\":\"inherit\",\"color\":\"inherit\"}'></span>\r\n\t\t\t    <span :style='{\"color\":\"inherit\",\"lineHeight\":\"1.5\",\"fontSize\":\"inherit\"}'>{{detail.storeupnum}}</span>\r\n\t\t\t  </div>\r\n\t\t\t  <div :style='{\"padding\":\"0\",\"margin\":\"0 10px 0 0\"}'>\r\n\t\t\t    <span class=\"icon iconfont icon-liulan13\" :style='{\"margin\":\"0 2px 0 0\",\"lineHeight\":\"1.5\",\"fontSize\":\"inherit\",\"color\":\"inherit\"}'></span>\r\n\t\t\t    <span :style='{\"color\":\"inherit\",\"lineHeight\":\"1.5\",\"fontSize\":\"inherit\"}'>{{detail.clicknum}}</span>\r\n\t\t\t  </div>\r\n\t\t\t</div>\r\n\t\t\t<div class=\"operate\" :style='{\"padding\":\"10px 0px\",\"alignItems\":\"center\",\"color\":\"#666\",\"display\":\"flex\",\"width\":\"100%\",\"fontSize\":\"14px\",\"justifyContent\":\"center\",\"order\":\"2\"}'>\r\n\t\t\t\t<div @click=\"zanClick\" :style='{\"border\":\"0px solid #eee\",\"cursor\":\"pointer\",\"padding\":\"0 15px\",\"margin\":\"0 20px 0 0\",\"alignItems\":\"center\",\"borderRadius\":\"20px\",\"display\":\"flex\",\"justifyContent\":\"center\"}'>\r\n\t\t\t\t\t<span class=\"icon iconfont icon-zan07\" :style='{\"margin\":\"0 2px 0 0\",\"lineHeight\":\"30px\",\"fontSize\":\"24px\",\"color\":\"#0adc91\"}'></span>\r\n\t\t\t\t\t<span :style='{\"color\":\"inherit\",\"lineHeight\":\"30px\",\"fontSize\":\"inherit\"}'>{{zanType?'取消赞':'点赞'}}</span>\r\n\t\t\t\t</div>\r\n\t\t\t\t<div @click=\"collectClick\" :style='{\"border\":\"0px solid #eee\",\"cursor\":\"pointer\",\"padding\":\"0 15px\",\"alignItems\":\"center\",\"borderRadius\":\"20px\",\"display\":\"flex\",\"justifyContent\":\"center\"}'>\r\n\t\t\t\t\t<span class=\"icon iconfont icon-shoucang04\" :style='{\"margin\":\"0 10px 0 0\",\"lineHeight\":\"30px\",\"fontSize\":\"24px\",\"color\":\"#ff777f\"}'></span>\r\n\t\t\t\t\t<span :style='{\"color\":\"inherit\",\"lineHeight\":\"30px\",\"fontSize\":\"inherit\"}'>{{collectType?'取消收藏':'收藏'}}</span>\r\n\t\t\t\t</div>\r\n\t\t\t</div>\r\n\t\t\t\r\n\t\t\t<div :style='{\"margin\":\"20px 0 0\",\"color\":\"#666\",\"width\":\"100%\",\"fontSize\":\"14px\",\"lineHeight\":\"1.8\",\"textIndent\":\"2em\",\"height\":\"auto\"}' class=\"content-detail\" v-html=\"detail.content\"></div>\r\n\t\t</div>\r\n\t\t<!-- option -->\r\n\t\t<div :style='{\"padding\":\"0 0 0 280px\",\"margin\":\"10px 0 0\",\"display\":\"flex\",\"width\":\"calc(100% - 0px)\",\"justifyContent\":\"space-between\",\"height\":\"auto\",\"order\":\"10\"}'>\r\n\t\t  <div :style='{\"cursor\":\"pointer\",\"margin\":\"0 50px 0 0\",\"lineHeight\":\"40px\",\"fontSize\":\"14px\",\"color\":\"#999\"}' @click=\"prepDetailClick\">上一篇：prev</div>\r\n\t\t  <div :style='{\"cursor\":\"pointer\",\"lineHeight\":\"40px\",\"fontSize\":\"14px\",\"color\":\"#999\"}' @click=\"nextDetailClick\">下一篇：next</div>\r\n\t\t</div>\r\n\r\n\t\t<!-- 推荐文章 -->\r\n\t\t<div class=\"news\" :style='{\"border\":\"0px solid #e9e9e9\",\"margin\":\"5px 20px 0 0\",\"textAlign\":\"center\",\"background\":\"#fff\",\"width\":\"260px\",\"height\":\"auto\",\"order\":\"1\"}'>\r\n\t\t  <div :style='{\"padding\":\"0px 20px\",\"color\":\"#fff\",\"textAlign\":\"left\",\"background\":\"rgb(22, 159, 25)\",\"display\":\"inline-block\",\"width\":\"100%\",\"lineHeight\":\"43px\",\"fontSize\":\"18px\"}'>推荐文章</div>\r\n\t\t  <div :style='{\"padding\":\"0\",\"margin\":\"0\",\"flexWrap\":\"wrap\",\"background\":\"#fff\",\"display\":\"flex\",\"width\":\"100%\",\"justifyContent\":\"space-between\",\"height\":\"auto\"}'>\r\n\t\t    <div v-for=\"item in recommendList\" :key=\"item.id\" :style='{\"cursor\":\"pointer\",\"width\":\"100%\",\"background\":\"#fff\",\"height\":\"auto\"}' @click=\"toDetail(item.id)\">\r\n\t\t      <img :style='{\"width\":\"100%\",\"objectFit\":\"cover\",\"display\":\"none\",\"height\":\"100px\"}' :src=\"baseUrl + item.picture\" alt=\"\">\r\n\t\t      <div :style='{\"padding\":\"0 10px\",\"borderColor\":\"#eee\",\"overflow\":\"hidden\",\"whiteSpace\":\"nowrap\",\"color\":\"#666\",\"textAlign\":\"left\",\"borderWidth\":\"1px 0 0\",\"width\":\"100%\",\"lineHeight\":\"50px\",\"fontSize\":\"14px\",\"textOverflow\":\"ellipsis\",\"borderStyle\":\"solid\"}'>{{ item.title }}</div>\r\n\t\t      <div :style='{\"padding\":\"0 10px\",\"lineHeight\":\"24px\",\"fontSize\":\"14px\",\"color\":\"#999\",\"textAlign\":\"right\",\"display\":\"none\"}'>{{item.addtime}}</div>\r\n\t\t    </div>\r\n\t\t  </div>\r\n\t\t</div>\r\n\t</div>\r\n\t</div>\r\n</template>\r\n\r\n<script>\r\n\texport default {\r\n\t\t//数据集合\r\n\t\tdata() {\r\n\t\t\treturn {\r\n\t\t\t\tid: 0,\r\n\t\t\t\tdetail: {},\r\n\t\t\t\tzanType: false,\r\n\t\t\t\tzanForm: {},\r\n\t\t\t\tcollectType:false,\r\n\t\t\t\tcollectForm: {},\r\n\t\t\t\tbaseUrl:'',\r\n\t\t\t\trecommendList: [],\r\n\t\t\t\tcategoryList: [],\r\n\t\t\t\tcurrentIndex: 0,\r\n\t\t\t\tallList: []\r\n\t\t\t}\r\n\t\t},\r\n\t\tcreated() {\r\n\t\t\tthis.id = this.$route.query.id\r\n\t\t\tthis.baseUrl = this.$config.baseUrl;\r\n\t\t\tthis.getCategoryList()\r\n\t\t\tthis.getDetail()\r\n\t\t\tthis.getNewsList()\r\n\t\t\tthis.getRecommendList()\r\n\t\t},\r\n\t\twatch:{\r\n\t\t\t$route(newValue) {\r\n\t\t\t\tthis.id = this.$route.query.id\r\n\t\t\t    this.getDetail()\r\n\t\t\t}\r\n\t\t},\r\n\t\tmethods: {\r\n\t\t\t  backClick() {\r\n\t\t\t\t  history.back()\r\n\t\t\t  },\r\n\t\t\tgetNewsList() {\r\n\t\t\t\tlet params = {page:1, limit: 100,sort:'addtime',order:'desc'};\r\n\t\t\t\tthis.$http.get('news/list', {params: params}).then(res => {\r\n\t\t\t\t\tif (res.data.code == 0) {\r\n\t\t\t\t\t\tfor(let x in res.data.data.list){\r\n\t\t\t\t\t\t\tif(res.data.data.list[x].id == this.id){\r\n\t\t\t\t\t\t\t\tthis.currentIndex = Number(x)\r\n\t\t\t\t\t\t\t\tbreak\r\n\t\t\t\t\t\t\t}\r\n\t\t\t\t\t\t}\r\n\t\t\t\t\t\tthis.allList = res.data.data.list\r\n\t\t\t\t\t}\r\n\t\t\t\t});\r\n\t\t\t},\r\n\t\t\t// 上一篇\r\n\t\t\tprepDetailClick(){\r\n\t\t\t\tif(this.currentIndex == 0){\r\