splay\":\"inline-block\"}' label=\"症状描述\" prop=\"zhengzhuangmiaoshu\">\r\n            <el-input\r\n              type=\"textarea\"\r\n              :rows=\"8\"\r\n              placeholder=\"症状描述\"\r\n              v-model=\"ruleForm.zhengzhuangmiaoshu\">\r\n            </el-input>\r\n          </el-form-item>\r\n\r\n      <el-form-item :style='{\"padding\":\"0\",\"margin\":\"0\"}'>\r\n        <el-button :style='{\"border\":\"0\",\"cursor\":\"pointer\",\"padding\":\"0\",\"margin\":\"0 20px 0 0\",\"outline\":\"none\",\"color\":\"#333\",\"borderRadius\":\"0px\",\"background\":\"#b4ebcc\",\"width\":\"100px\",\"lineHeight\":\"36px\",\"fontSize\":\"14px\",\"height\":\"36px\"}'  type=\"primary\" @click=\"onSubmit\">提交</el-button>\r\n        <el-button :style='{\"border\":\"1px solid #eee\",\"cursor\":\"pointer\",\"padding\":\"0\",\"margin\":\"0\",\"outline\":\"none\",\"color\":\"#333\",\"borderRadius\":\"0px\",\"background\":\"none\",\"width\":\"100px\",\"lineHeight\":\"36px\",\"fontSize\":\"14px\",\"height\":\"36px\"}' @click=\"back()\">返回</el-button>\r\n      </el-form-item>\r\n    </el-form>\r\n</div>\r\n</template>\r\n\r\n<script>\r\n  export default {\r\n    data() {\r\n\t  let self = this\r\n      return {\r\n        id: '',\r\n        baseUrl: '',\r\n        ro:{\r\n\t\t\t\tyishenggonghao : false,\r\n\t\t\t\tyishengxingming : false,\r\n\t\t\t\ttouxiang : false,\r\n\t\t\t\tkeshi : false,\r\n\t\t\t\twenzhenshijian : false,\r\n\t\t\t\tzhengzhuangmiaoshu : false,\r\n\t\t\t\thuanzhezhanghao : false,\r\n\t\t\t\thuanzhexingming : false,\r\n\t\t\t\txingbie : false,\r\n\t\t\t\tnianling : false,\r\n\t\t\t\tshhf : false,\r\n        },\r\n        type: '',\r\n        userTableName: localStorage.getItem('UserTableName'),\r\n        ruleForm: {\r\n          yishenggonghao: '',\r\n          yishengxingming: '',\r\n          touxiang: '',\r\n          keshi: '',\r\n          wenzhenshijian: '',\r\n          zhengzhuangmiaoshu: '',\r\n          huanzhezhanghao: '',\r\n          huanzhexingming: '',\r\n          xingbie: '',\r\n          nianling: '',\r\n        },\r\n\r\n\r\n        rules: {\r\n          yishenggonghao: [\r\n          ],\r\n          yishengxingming: [\r\n          ],\r\n          touxiang: [\r\n          ],\r\n          keshi: [\r\n          ],\r\n          wenzhenshijian: [\r\n          ],\r\n          zhengzhuangmiaoshu: [\r\n          ],\r\n          huanzhezhanghao: [\r\n          ],\r\n          huanzhexingming: [\r\n          ],\r\n          xingbie: [\r\n          ],\r\n          nianling: [\r\n            { validator: this.$validate.isIntNumer, trigger: 'blur' },\r\n          ],\r\n          shhf: [\r\n          ],\r\n        },\r\n\t\tcenterType: false,\r\n      };\r\n    },\r\n    computed: {\r\n\r\n\r\n\r\n    },\r\n    components: {\r\n    },\r\n    created() {\r\n\t\tif(this.$route.query.centerType){\r\n\t\t\tthis.centerType = true\r\n\t\t}\r\n\t  //this.bg();\r\n      let type = this.$route.query.type ? this.$route.query.type : '';\r\n      this.init(type);\r\n      this.baseUrl = this.$config.baseUrl;\r\n      this.ruleForm.wenzhenshijian = this.getCurDate()\r\n    },\r\n    methods: {\r\n      getMakeZero(s) {\r\n          return s < 10 ? '0' + s : s;\r\n      },\r\n      // 下载\r\n      download(file){\r\n        window.open(`${file}`)\r\n      },\r\n      // 初始化\r\n      init(type) {\r\n        this.type = type;\r\n        if(type=='cross'){\r\n          var obj = JSON.parse(localStorage.getItem('crossObj'));\r\n          for (var o in obj){\r\n            if(o=='yishenggonghao'){\r\n              this.ruleForm.yishenggonghao = obj[o];\r\n              this.ro.yishenggonghao = true;\r\n              continue;\r\n            }\r\n            if(o=='yishengxingming'){\r\n              this.ruleForm.yishengxingming = obj[o];\r\n              this.ro.yishengxingming = true;\r\n              continue;\r\n            }\r\n            if(o=='touxiang'){\r\n              this.ruleForm.touxiang = obj[o].split(\",\")[0];\r\n              this.ro.touxiang = true;\r\n              continue;\r\n            }\r\n            if(o=='keshi'){\r\n              this.ruleForm.keshi = obj[o];\r\n              this.ro.keshi = true;\r\n              continue;\r\n            }\r\n            if(o=='wenzhenshijian'){\r\n              this.ruleForm.wenzhenshijian = obj[o];\r\n              this.ro.wenzhenshijian = true;\r\n              continue;\r\n            }\r\n            if(o=='zhengzhuangmiaoshu'){\r\n              this.ruleForm.zhengzhuangmiaoshu = obj[o];\r\n              this.ro.zhengzhuangmiaoshu = true;\r\n              continue;\r\n            }\r\n            if(o=='huanzhezhanghao'){\r\n              this.ruleForm.huanzhezhanghao = obj[o];\r\n              this.ro.huanzhezhanghao = true;\r\n              continue;\r\n            }\r\n            if(o=='huanzhexingming'){\r\n              this.ruleForm.huanzhexingming = obj[o];\r\n              this.ro.huanzhexingming = true;\r\n              continue;\r\n            }\r\n            if(o=='xingbie'){\r\n              this.ruleForm.xingbie = obj[o];\r\n              this.ro.xingbie = true;\r\n              continue;\r\n            }\r\n            if(o=='nianling'){\r\n              this.ruleForm.nianling = obj[o];\r\n              this.ro.nianling = true;\r\n              continue;\r\n            }\r\n          }\r\n        }else if(type=='edit'){\r\n\t\t\tthis.info()\r\n\t\t}\r\n        // 获取用户信息\r\n        this.$http.get(this.userTableName + '/session', {emulateJSON: true}).then(res => {\r\n          if (res.data.code == 0) {\r\n            var json = res.data.data;\r\n            if((json.huanzhezhanghao!=''&&json.huanzhezhanghao) || json.huanzhezhanghao==0){\r\n                this.ruleForm.huanzhezhanghao = json.huanzhezhanghao;\r\n\t\t\t\tthis.ro.huanzhezhanghao = true;\r\n            }\r\n            if((json.huanzhexingming!=''&&json.huanzhexingming) || json.huanzhexingming==0){\r\n                this.ruleForm.huanzhexi